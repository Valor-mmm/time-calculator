  "devDependencies": {
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "autoprefixer": "10.2.6",
    "eslint": "7.32.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
